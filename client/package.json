{
  "name": "spring-boot-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.19.0",
    "babel-polyfill": "6.26.0",
    "chart.js": "2.7.3",
    "formik": "^1.5.1",
    "fullcalendar": "4.0.0-alpha.2",
    "primeflex": "1.0.0-rc.1",
    "primeicons": "1.0.0",
    "primereact": "^3.1.3",
    "react": "^16.8.5",
    "react-dom": "^16.8.5",
    "react-router-dom": "^5.0.0",
    "react-scripts": "5.0.1",
    "react-transition-group": "^2.5.1"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/layout/ -o src/layout/",
    "analyze": "source-map-explorer build/static/js/main.*",
    "start": "react-scripts start",
    "build": "react-scripts build && react-snapshot",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "clear-babel-cache": "rm -rf ./node_modules/.cache/babel-loader/*",
    "format": "prettier --write src/**/*.{js,jsx}"
  },
  "prettier": {
    "printWidth": 100,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none",
    "tabWidth": 2
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@bufferapp/components": "^3.2.1",
    "classnames": "^2.2.6",
    "node-sass-chokidar": "^1.3.0",
    "npm-run-all": "^4.1.3",
    "react-snapshot": "^1.3.0",
    "yup": "^0.27.0"
  }
}
